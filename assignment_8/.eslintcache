                           \"line_linked\": {\n                                        \"enable\": false\n                                    },\n                                    shape: {\n                                        type: 'image',\n                                        \"image\": [\n                                            {\n                                                \"src\": \"/icons/github.svg\",\n                                            },\n                                            {\n                                                \"src\": \"/icons/twitter.svg\",\n                                            },\n                                            {\n                                                \"src\": \"/icons/instagram.svg\",\n                                            },\n                                            {\n                                                \"src\": \"/icons/linkedin.svg\",\n                                            },\n                                            {\n                                                \"src\": \"/icons/youtube.svg\",\n                                            },\n                                            {\n                                                \"src\": \"/icons/weibo.svg\",\n                                            },\n                                        ]\n                                    },\n                                    \"size\": {\n                                        \"value\": 30,\n                                        \"random\": false,\n                                        \"anim\": {\n                                            \"enable\": true,\n                                            \"speed\": 1,\n                                            \"size_min\": 20,\n                                            \"sync\": false\n                                        }\n                                    }\n       